---
import '../../node_modules/paf-viewer/dist/style.css';
---

<script>
import {GalleryScene} from 'paf-viewer'
import galleryDataFile from '../assets/data.json' 
if (galleryDataFile) {
	galleryDataFile.allResources.map((resource) => {
		resource.url = 'https://docs.peopleartfactory.com/universoitalocalvino/' + resource.fileName;
	})
}
GalleryScene.instance.load(galleryDataFile,{ CONTENT_DISTANCE:200});
</script>
